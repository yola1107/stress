{
  "dashboard": {
    "id": 6,
    "title": "stress-service",
    "tags": ["stress-test", "prometheus"],
    "timezone": "",
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "templating": {
      "list": [
        {
          "name": "task_id",
          "label": "任务 ID",
          "type": "query",
          "query": "stress_task_qps",
          "refresh": 2,
          "sort": 1,
          "regex": "/.*task_id=\"([^\"]+)\".*/",
          "multi": false,
          "includeAll": false,
          "current": {}
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "QPS 实时监控",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "stress_task_qps{task_id=\"$task_id\"}",
            "legendFormat": "{{task_id}} QPS",
            "refId": "A",
            "interval": "5s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            },
            "custom": {
              "axisLabel": "QPS",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          },
          "overrides": []
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "list",
            "placement": "bottom",
            "calcs": ["last", "min", "max", "mean"]
          }
        }
      },
      {
        "id": 2,
        "title": "QPS 统计卡片",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "stress_task_qps{task_id=\"$task_id\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            },
            "mappings": []
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["last"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "wideLayout": true,
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "平均 QPS (5分钟)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "avg_over_time(stress_task_qps{task_id=\"$task_id\"}[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["last"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "wideLayout": true,
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 4,
        "title": "QPS 峰值",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "max_over_time(stress_task_qps{task_id=\"$task_id\"}[15m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "value"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["last"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "wideLayout": true,
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 5,
        "title": "QPS 趋势",
        "type": "timeseries",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "rate(stress_task_qps{task_id=\"$task_id\"}[1m])",
            "legendFormat": "QPS 变化率",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "showLegend": false,
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      }
    ]
  }
}